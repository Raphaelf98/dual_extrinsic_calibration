<launch>
  <!--Launch calibration tool. Adjust launch parmameters according to camera image and camera topic names.-->
  <node pkg="dual_extrinsic_calibration" type="dual_extrinsic_calibration_node" name="dual_extrinsic_calibration_node"  output="screen"/>
    <!-- <param name="camera_1_image" type="string" value="/935322071831/color/image_raw" /> -->
    <!-- <param name="camera_1_image" type="string" value="/939622074287/color/image_raw" /> -->
    <!-- <param name="camera_2_image" type="string" value="/usb_cam/image_raw" /> -->
    <param name="camera_2_image" type="string" value="/camera/color/image_raw" />
    <param name="camera_1_image" type="string" value="/camera/rgb/image_mono" />

    <!-- <param name="camera_1_info"  type="string" value="/935322071831/color/camera_info"/> -->
    <!-- <param name="camera_1_info"  type="string" value="/939622074287/color/camera_info" /> -->

    <!-- <param name="camera_2_info"  type="string" value="/usb_cam/camera_info" /> -->
    <param name="camera_2_info"  type="string" value="/camera/color/camera_info" />
    <param name="camera_1_info" type="string" value="/camera/rgb/camera_info" />
     <!--Chessboard dimensions-->
    <param name="chessboard_size_a" type="int" value="8" />
    <param name="chessboard_size_b" type="int" value="5" />
    <param name="chessboard_length" type="double" value="0.03" />
    <!--Number of samples to take before computing average transform-->
    <param name="sample_size" type="int" value="10" />
  <!--Launch TF publisher to publish camera pose relative to parent frame (provide optical axis frame)-->
  <node pkg="dual_extrinsic_calibration" type="tfPosePublisher" name="tf_pose_broadcaster"/>
    <!-- <param name="parent_optical_frame" value="/939622074287_color_optical_frame"/> -->
    <param name="parent_optical_frame" value="/camera_rgb_optical_frame"/>
  <!-- <node type="rviz" name="rviz" pkg="rviz" args="-d $(find dual_extrinsic_calibration)/config/realsense.rviz"/> -->
  <node type="rviz" name="rviz" pkg="rviz" args="-d $(find dual_extrinsic_calibration)/config/usbcamrrealsense.rviz"/> 
</launch> 
